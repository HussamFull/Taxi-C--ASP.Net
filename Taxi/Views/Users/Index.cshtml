@{
}


<!DOCTYPE html>
<html>
<head>
	<title> All Users </title>
</head>
<body>

	<h1>Users</h1>
	<table>
		<thead>
			<tr>
				<th>Id</th>
				<th> Name</th>

				<th>Email</th>
				<th>Phone Number</th>
			</tr>
		</thead>
		<tbody>
			@foreach (var user in Model)
			{
				<tr>
					<td>@user.Id</td>
					<td>@user.Name</td>

					<td>@user.Email</td>
					<td>@user.PhoneNumber</td>
				</tr>
			}
		</tbody>
	</table>

</body>
</html>